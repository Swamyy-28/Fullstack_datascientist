   "source": [
    "num = int(input(\"Enter a number: \"))\n",
    "rev = 0\n",
    "temp = num\n",
    "while temp > 0:\n",
    "    rev = rev * 10 + temp % 10\n",
    "    temp //= 10\n",
    "print(\"Reversed:\", rev)\n"
   ]
  }
