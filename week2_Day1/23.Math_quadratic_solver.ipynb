   "source": [
    "import math\n",
    "\n",
    "def quadratic(a,b,c):\n",
    "    d = b**2 - 4*a*c\n",
    "    r1 = (-b + math.sqrt(d))/(2*a)\n",
    "    r2 = (-b - math.sqrt(d))/(2*a)\n",
    "    return r1, r2\n",
    "\n",
    "print(\"Roots are:\", quadratic(1,-3,2))\n"
   ]
  }
