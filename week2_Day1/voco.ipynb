    "def count(str):\n",
    "    c=0\n",
    "    v=0\n",
    "    str=str.strip().lower()\n",
    "    ch='AEIOUaeiou'\n",
    "    for char in str:\n",
    "        if char.isalpha(): \n",
    "            if char in ch:\n",
    "                v+= 1\n",
    "            else:\n",
    "                c+= 1\n",
    "    print(\"vowel\",v)\n",
    "    print(\"cons\",c)\n",
    "count(\"Hello\")\n"
   ]
  }
